@using System.Web.Mvc.Html
@model Web.ViewModels.TypeViewModel

<div class="background-cover">
    <div>@Html.ActionLink("Add", "Add", "MediaType", null, null)</div>

    @foreach (var entity in Model.Types)
    {
<div style="border: 1px solid red; width: 100px; height: 100px;">
    <div>@entity.Name</div>
    <div>@Html.ActionLink("Delete", "Delete", "MediaType", new { id = entity.Id }, null)</div>
    <div>@Html.ActionLink("Edit", "Edit", "MediaType", new { id = entity.Id }, null)</div>
</div>}

    @Html.Partial("Partials/PagePartial/TypePagePartial")
</div>

